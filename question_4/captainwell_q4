def fabonacci(fab=[]):    try:        n = int(input("请输入变量n:"))        if n < 0:            print("请输入大于等于0的整数")            return    except ValueError:        print("请输入大于等于0的整数")        return    for item in range(0,n+1):        if item == 0:            fab.append(0)        elif item == 1:            fab.append(1)        elif n > 1:            fab.append(fab[item-1] + fab[item-2])    print("您需要的斐波那契数列为")    for item in range(0,n+1):        print(fab[item],end=" ")if __name__ == '__main__':    fabo = []    fabonacci(fabo)
